#!/export/home/ricardo/projects/pip-chill/env/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'pip-chill==0.1.9','console_scripts','pip-chill'
__requires__ = 'pip-chill==0.1.9'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pip-chill==0.1.9', 'console_scripts', 'pip-chill')()
    )
