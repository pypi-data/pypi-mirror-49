{
    ">": {
        "action": "Increment data pointer",
        "substitute": "[:...:...]"
    },
    "<": {
        "action": "Decrement data pointer",
        "substitute": "[...:...]"
    },

    "+": {
        "action": "Increment pointee",
        "substitute": "[::...]"
    },
    "-": {
        "action": "Decrement pointee",
        "substitute": "[...:]"
    },

    ".": {
        "action": "Print pointee as ascii character",
        "substitute": "[:]"
    },
    ",": {
        "action": "Input an ascii character into pointee",
        "substitute": "[...:...:...]"
    },

    "[": {
        "action": "Jump to corresponded close brace when pointee is zero",
        "substitute": "[:...]"
    },
    "]": {
        "action": "Jump to corresponded open brace when pointee is not zero",
        "substitute": "[...::...]"
    }
}
