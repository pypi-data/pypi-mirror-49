<?xml version="1.0" encoding="UTF-8"?>
<jnlp spec="1.0+">
    <information>
        <title>DavLink</title>
        <vendor>Luc Saffre</vendor>
        <offline-allowed/>
    </information>
    <security><all-permissions/></security>
    <resources>
        <!-- Application Resources -->
        <j2se version="1.7+" />
        <jar href="{{davlink.build_lib_url('DavLink.jar')}}" 
             main="true"/>
        <!-- href attribute contains relative path;
        codebase attribute not specified -->
    </resources>
    <applet-desc
         name="DavLink"
         main-class="davlink.DavLink"
         width="1"
         height="1">
     </applet-desc>
     <update check="background"/>
</jnlp>
