{
    "plugin": {
        "title": "Document DB",
        "packageName": "peek_plugin_docdb",
    "version":"1.3.10",
        "buildNumber": "#PLUGIN_BUILD#",
        "buildDate": "#BUILD_DATE#",
        "creator": "Synerty Pty Ltd",
        "website": "www.synerty.com"
    },
    "requiresServices": [
        "server",
        "worker",
        "agent",
        "storage",
        "admin",
        "client",
        "mobile",
        "desktop",
        "doc-dev",
        "doc-admin"
    ],
    "doc-dev": {
        "docDir": "doc-dev",
        "docRst": "index.rst",
        "hasApi": true
    },
    "doc-admin": {
        "docDir": "doc-admin",
        "docRst": "index.rst"
    },
    "server": {
    },
    "storage": {
        "alembicDir": "_private/alembic"
    },
    "admin": {
        "showHomeLink": true,
        "appDir": "_private/admin-app",
        "appModule": "docDb.module#DocDbModule",
        "moduleDir": "plugin-module"
    },
    "client": {
    },
    "mobile": {
        "cfgDir": "_private/both-cfg",
        "cfgModule": "docdb-cfg.module#DocDBCfgModule",
        "showHomeLink": false,
        "appDir": "_private/both-app",
        "appModule": "docdb.module#DocDbModule",
        "assetDir": "_private/both-assets",
        "icon": "/assets/peek_plugin_docdb/icon.png",
        "moduleDir": "plugin-module",
        "rootServices": [
            {
                "file": "_private/DocDbTupleService",
                "class": "DocDbTupleService"
            },
            {
                "file": "_private/document-loader/PrivateDocumentLoaderService",
                "class": "PrivateDocumentLoaderService",
                "persistent": true
            },
            {
                "file": "DocDbService",
                "class": "DocDbService"
            }
        ]
    },
    "desktop": {
        "cfgDir": "_private/both-cfg",
        "cfgModule": "docdb-cfg.module#DocDBCfgModule",
        "showHomeLink": false,
        "appDir": "_private/both-app",
        "appModule": "docdb.module#DocDbModule",
        "assetDir": "_private/both-assets",
        "icon": "/assets/peek_plugin_docdb/icon.png",
        "moduleDir": "plugin-module",
        "rootServices": [
            {
                "file": "_private/DocDbTupleService",
                "class": "DocDbTupleService"
            },
            {
                "file": "_private/document-loader/PrivateDocumentLoaderService",
                "class": "PrivateDocumentLoaderService",
                "persistent": true
            },
            {
                "file": "DocDbService",
                "class": "DocDbService"
            }
        ]
    }
}
